<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.feedforfree">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FeedForFree">
        <activity
            android:name=".User.thankyou"
            android:parentActivityName=".User.UserDashboard"
            />


        <activity
            android:name=".User.deliverydetails"
            android:parentActivityName=".User.UserDashboard"/>
        <activity
            android:name=".User.ContactUs"
            android:parentActivityName=".User.Profilemenu" />
        <activity
            android:name=".User.Profilemenu"
            android:parentActivityName=".User.UserDashboard" />
        <activity
            android:name=".User.Favourite"
            android:parentActivityName=".User.UserDashboard" />
        <activity
            android:name=".User.Resetpassword"
            android:parentActivityName=".User.Profilemenu" />
        <activity android:name=".User.register" />
        <activity android:name=".User.Login" />
        <activity android:name=".User.UserDashboard" />
        <activity android:name=".common.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>